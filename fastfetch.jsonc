{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "file",
    "source": "~/.config/omarchy/branding/about.txt",
    "color": { "1": "#cba6f7" },
    "padding": {
      "top": 2,
      "right": 6,
      "left": 2
    }
  },
  "modules": [
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;203;166;247m┌──────────────────────Hardware──────────────────────┐\u001b[0m"
    },
    {
      "type": "host",
      "key": " PC",
      "keyColor": "#a6e3a1"
    },
    {
      "type": "cpu",
      "key": "│ ├",
      "showPeCoreCount": true,
      "keyColor": "#a6e3a1"
    },
    {
      "type": "gpu",
      "key": "│ ├",
      "detectionMethod": "pci",
      "keyColor": "#a6e3a1"
    },
    {
      "type": "display",
      "key": "│ ├󱄄",
      "keyColor": "#a6e3a1"
    },
    {
      "type": "disk",
      "key": "│ ├󰋊",
      "keyColor": "#a6e3a1"
    },
    {
      "type": "memory",
      "key": "│ ├",
      "keyColor": "#a6e3a1"
    },
    {
      "type": "swap",
      "key": "└ └󰓡 ",
      "keyColor": "#a6e3a1"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;203;166;247m└────────────────────────────────────────────────────┘\u001b[0m"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;203;166;247m┌──────────────────────Software──────────────────────┐\u001b[0m"
    },
    {
      "type": "command",
      "key": "\ue900 OS",
      "keyColor": "#74c7ec",
      "text": "version=$(omarchy-version); echo \"Omarchy $version\""
    },
    {
      "type": "command",
      "key": "│ ├󰘬",
      "keyColor": "#74c7ec",
      "text": "branch=$(omarchy-version-branch); echo \"$branch\""
    },
    {
      "type": "command",
      "key": "│ ├󰔫",
      "keyColor": "#74c7ec",
      "text": "channel=$(omarchy-version-channel); echo \"$channel\""
    },
    {
      "type": "kernel",
      "key": "│ ├",
      "keyColor": "#74c7ec"
    },
    {
      "type": "wm",
      "key": "│ ├",
      "keyColor": "#74c7ec"
    },
    {
      "type": "de",
      "key": " DE",
      "keyColor": "#74c7ec"
    },
    {
      "type": "terminal",
      "key": "│ ├",
      "keyColor": "#74c7ec"
    },
    {
      "type": "packages",
      "key": "│ ├󰏖",
      "keyColor": "#74c7ec"
    },
    {
      "type": "wmtheme",
      "key": "│ ├󰉼",
      "keyColor": "#74c7ec"
    },
    {
      "type": "command",
      "key": "│ ├󰸌",
      "keyColor": "#74c7ec",
      "text": "theme=$(omarchy-theme-current); echo -e \"$theme \\e[38;2;203;166;247m●\\e[38;2;245;194;231m●\\e[38;2;116;199;236m●\\e[38;2;166;227;161m●\\e[38;2;249;226;175m●\\e[38;2;243;139;168m●\\e[0m\""
    },
    {
      "type": "terminalfont",
      "key": "└ └",
      "keyColor": "#74c7ec"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;203;166;247m└────────────────────────────────────────────────────┘\u001b[0m"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;203;166;247m┌────────────────Age / Uptime / Update───────────────┐\u001b[0m"
    },
    {
      "type": "command",
      "key": "󱦟 OS Age",
      "keyColor": "#fab387",
      "text": "echo $(( ($(date +%s) - $(stat -c %W /)) / 86400 )) days"
    },
    {
      "type": "uptime",
      "key": "󱫐 Uptime",
      "keyColor": "#fab387"
    },
    {
      "type": "command",
      "key": " Update",
      "keyColor": "#fab387",
      "text": "updated=$(omarchy-version-pkgs); echo \"$updated\""
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;203;166;247m└────────────────────────────────────────────────────┘\u001b[0m"
    },
    "break"
  ]
}
